<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="cpv_CircularProgressView">
        <attr name="cpv_strokeSize" format="dimension"/>
        <attr name="cpv_strokeColor" format="color"/>

        <attr name="cpv_startDrawable" format="color|reference"/>
        <attr name="cpv_progressDrawable" format="color|reference"/>
        <attr name="cpv_endDrawable" format="color|reference"/>

        <attr name="cpv_startDrawableMargins" format="dimension"/>
        <attr name="cpv_progressDrawableMargins" format="dimension"/>
        <attr name="cpv_endDrawableMargins" format="dimension"/>
    </declare-styleable>
</resources>
